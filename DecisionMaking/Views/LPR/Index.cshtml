@model IEnumerable<DecisionMaking.Models.LPR>

@{
    ViewBag.Title = "LPR";
}

<h2>LPR</h2>

<p>
    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-success" })
</p>
<table class="table table-striped table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LRange)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LRange)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.LNum }) |
            @Html.ActionLink("Details", "Details", new { id=item.LNum }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.LNum })
        </td>
    </tr>
}

</table>
