@model IEnumerable<DecisionMaking.Models.Vector>

@{
    ViewBag.Title = "Vector";
}

<h2>Vector</h2>

<p>
    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-success" })
</p>
<table class="table table-striped table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Alternative.AName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mark.MName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Alternative.AName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mark.MName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.VNum }) |
            @Html.ActionLink("Details", "Details", new { id=item.VNum }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.VNum })
        </td>
    </tr>
}

</table>
